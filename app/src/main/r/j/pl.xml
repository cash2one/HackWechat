<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:drawable="@color/qm" />
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/aa" />
            <corners android:topLeftRadius="0dp" android:topRightRadius="@dimen/d4" android:bottomLeftRadius="0dp" android:bottomRightRadius="@dimen/d4" />
